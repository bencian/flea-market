<div id='flash_messages_container'>
  <% flash.each do |key, value| %>
        <div class="alert alert-<%= get_flash_class(key) %>">
          <%= value %>
          <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
  <% end %>
</div>