<h1 class='mt-2'><%= t('entities.categories') %></h1>
<%= render 'flash_message' %>

<%= render 'filter' %>

<table class="table table-bordered">
  <thead class="thead-light">
    <tr>
      <th>Nombre</th>
      <th class='text-right'><%= link_to t('button_options.new'), new_admin_category_path, class: 'btn btn-primary' %></th>
    </tr>
  </thead>
  <tbody>
    <%= render @presenter.categories %>
  </tbody>
</table>

<%= will_paginate @presenter.categories %>