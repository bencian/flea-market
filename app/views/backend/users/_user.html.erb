<tr>
  <td><%= user.username %></td>
  <td><%= user.email %></td>
  <td class='text-right'>
    <% if current_admin == user %>
      <%= link_to t('button_options.edit'), edit_admin_registration_path, class: 'btn btn-sm btn-outline-warning' %>
    <% else %>
      <%= link_to t('button_options.edit'), edit_admin_user_path(user), class: 'btn btn-sm btn-outline-warning' %>
    <% end %>
    <%= link_to t('button_options.delete'), admin_user_path(user), method: :delete, data: { confirm: t('alerts.you_sure') }, class: 'btn btn-sm btn-outline-danger' %>
  </td>
</tr>

