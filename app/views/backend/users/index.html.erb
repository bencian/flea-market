<h1 class='mt-2'><%= t('entities.users')%></h1>
<%= render 'flash_message' %>

<%= render 'filter' %>

<table class="table table-bordered">
  <thead class="thead-light">
    <tr>
      <th><%= t('entity_fields.user.username')%></th>
      <th><%= t('entity_fields.user.email')%></th>
      <th class='text-right'><%= link_to t('button_options.new'), new_admin_user_path, class: 'btn btn-primary' %></th>
    </tr>
  </thead>
  <tbody>
    <%= render partial: "user", collection: @presenter.users %>
  </tbody>
</table>
